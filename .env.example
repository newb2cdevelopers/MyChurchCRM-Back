# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_SECRET=your-google-client-secret-here

# Cache Configuration
TIMEOUT_GENERAL_CACHE=300

# MongoDB Connection
DEV_DB_CONNECTION=mongodb://localhost:27017/your-database-name

# Email Service Configuration (Gmail SMTP)
# For Gmail: Use App Password, not your regular password
# Generate at: https://myaccount.google.com/apppasswords
USER_MAIL=your-email@gmail.com
PASS_MAIL=your-app-specific-password

# Frontend URL (for password recovery links)
APP_URL_BASE=http://localhost:3000

# Server Port
PORT=4000